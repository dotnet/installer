    <!--
      It's necessary to restore the packages in multiple passes because the NuGet logic is only capable of downloading
      one version of a given package name. But in some cases, we need multiple versions of a package. So for each package,
      we execute a separate restore.
     -->
    <MSBuild Projects="$(MSBuildThisFileFullPath)"
             Targets="Restore"
             Properties="PackageName=%(PackageDownload.Identity);PackageVersion=%(PackageDownload.Version)" />
  </Target>

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <Arch>$([System.Runtime.InteropServices.RuntimeInformation]::OSArchitecture)</Arch>
  </PropertyGroup>

  <Target Name="BeforeCollectPackageDownloads" BeforeTargets="CollectPackageDownloads">
    <Message Text="Restoring package $(PackageName).$(PackageVersion)..." Importance="high" />
    <ItemGroup>
      <PackageDownload Include="$(PackageName)" Version="[$(PackageVersion)]" />
    </ItemGroup>
  </Target>

</Project>
