// Container contains checked-out source code only
{
    "name": "Default",
    "image": "mcr.microsoft.com/dotnet-buildtools/prereqs:fedora-36",
    "hostRequirements": {
        // A completely source built .NET is >64 GB with all the repos/artifacts
        "storage": "128gb"
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-dotnettools.csharp",
                "eamodio.gitlens"
            ]
        }
    }
}